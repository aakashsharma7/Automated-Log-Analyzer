{
  "name": "automated-log-analyzer",
  "version": "1.0.0",
  "description": "Modern AI-powered log analysis application with intelligent parsing, anomaly detection, and real-time insights",
  "author": "Aakash Sharma",
  "license": "MIT",
  "private": true,
  "keywords": [
    "log-analyzer",
    "log-analysis",
    "anomaly-detection",
    "machine-learning",
    "nextjs",
    "react",
    "typescript",
    "ai",
    "devops",
    "monitoring"
  ],
  "scripts": {
    "dev": "next dev",
    "dev:turbo": "next dev --turbo",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start",
    "start:prod": "NODE_ENV=production next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "lint:strict": "next lint --strict",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "clean": "rm -rf .next out node_modules/.cache",
    "clean:all": "npm run clean && rm -rf node_modules package-lock.json",
    "prepare": "npm run db:generate",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:seed": "prisma db seed",
    "perf:audit": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "test": "echo \"No tests specified\" && exit 0",
    "postinstall": "npm run db:generate"
  },
  "dependencies": {
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "typescript": "^5",
    "tailwindcss": "^3.3.0",
    "autoprefixer": "^10.0.1",
    "postcss": "^8",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.294.0",
    "tailwind-merge": "^2.0.0",
    "tailwindcss-animate": "^1.0.7",
    "recharts": "^2.8.0",
    "chart.js": "^4.4.0",
    "react-chartjs-2": "^5.2.0",
    "d3": "^7.8.5",
    "@types/d3": "^7.4.3",
    "prisma": "^5.7.0",
    "@prisma/client": "^5.7.0",
    "pg": "^8.11.3",
    "@types/pg": "^8.10.9",
    "openai": "^4.20.1",
    "@google/generative-ai": "^0.1.3",
    "multer": "^1.4.5-lts.1",
    "@types/multer": "^1.4.11",
    "csv-parser": "^3.0.0",
    "papaparse": "^5.4.1",
    "@types/papaparse": "^5.3.14",
    "ml-matrix": "^6.10.4",
    "ml-isolation-forest": "^0.1.0",
    "natural": "^6.7.0",
    "@types/natural": "^5.1.3",
    "compromise": "^14.10.0",
    "date-fns": "^2.30.0",
    "zod": "^3.22.4",
    "react-hook-form": "^7.48.2",
    "@hookform/resolvers": "^3.3.2",
    "framer-motion": "^10.16.16",
    "react-dropzone": "^14.2.3"
  },
  "devDependencies": {
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "prettier": "^3.0.0",
    "@types/prettier": "^3.0.0",
    "cross-env": "^7.0.3",
    "lighthouse": "^11.4.0",
    "@next/bundle-analyzer": "^14.0.4",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/automate-log.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "next": {
    "productionBrowserSourceMaps": false
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}